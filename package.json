{
  "name": "raspberry-controller",
  "version": "0.0.1",
  "description": "",
  "author": "levabala <levabala@ya.ru>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist && rimraf .next",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:hmr": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "build": "nest build && cd src/client && next build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:typecheck": "tsc --project tsconfig.json",
    "test:lint": "eslint src/ --ext .jsx,.js,.ts,.tsx --color",
    "test:lint:fix": "eslint src/ --ext .jsx,.js,.ts,.tsx --color --fix",
    "test:prettier:check": "prettier --check \"./src/**/*.{js,jsx,json,ts,tsx}\"",
    "test:prettier:fix": "prettier --write \"./src/**/*.{js,jsx,json,ts,tsx}\"",
    "types:generate": "ts-node --transpile-only src/scripts/generate-typings.ts",
    "types:generate:watch": "ts-node --transpile-only src/scripts/generate-typings-watch.ts",
    "typeorm": "cross-env NODE_PATH=src ts-node -P tsconfig.json -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "migration:up": "yarn typeorm migration:run",
    "migration:down": "yarn typeorm migration:revert"
  },
  "dependencies": {
    "@apollo/gateway": "^0.34.0",
    "@nestjs/common": "^8.0.4",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.4",
    "@nestjs/graphql": "^8.0.2",
    "@nestjs/platform-express": "^8.0.4",
    "@nestjs/terminus": "^7.2.0",
    "@nestjs/throttler": "^2.0.0",
    "@nestjs/typeorm": "^8.0.1",
    "apollo-server-express": "2.x.x",
    "cross-env": "^7.0.3",
    "graphql": "^15.5.1",
    "graphql-tools": "^7.0.5",
    "next": "^11.0.1",
    "pg": "^8.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "ts-morph": "^11.0.3",
    "typeorm": "^0.2.34"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.36",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "run-script-webpack-plugin": "^0.0.11",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3",
    "webpack": "^5.46.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "main": "index.js"
}
